{
  "name": "snapdown",
  "version": "0.2.0",
  "private": true,
  "description": "Draw snapshot diagrams with plain text",
  "homepage": "http://maxg.github.io/snapdown/",
  "repository": "github:maxg/snapdown",
  "license": "MIT",
  "author": "Max Goldman <maxg@mit.edu>",
  "contributors": [
    "Aaron Zeng <a2z@mit.edu>"
  ],
  "scripts": {
    "gen-parser": "node -p 'require(\"./src/snapdown.peg.js\")' | pegjs -o dist/snapdown-parser.js",
    "test": "npm run gen-parser && mocha",
    "build": "npm run gen-parser && webpack --mode development snapdown=./src/index.js && webpack --mode production snapdown.min=./src/index.js",
    "start": "webpack -w"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "elkjs": "^0.6.2",
    "mocha": "^5.2.0",
    "pegjs": "^0.10.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "yaml-front-matter": "^4.1.0"
  }
}
